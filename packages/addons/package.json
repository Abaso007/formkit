{
  "name": "@formkit/addons",
  "type": "module",
  "version": "1.3.1",
  "description": "Optional features you can add to your FormKit inputs.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./css/multistep": "./dist/css/multistep.css",
    "./css/floatingLabels": "./dist/css/floatingLabels.css",
    "./*": "./dist/*"
  },
  "keywords": [
    "vue",
    "forms",
    "inputs",
    "validation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/formkit/formkit.git",
    "directory": "packages/addons"
  },
  "contributors": [
    "Justin Schroeder <justin@formkit.com>",
    "Andrew Boyd <andrew@formkit.com>"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "@formkit/auto-animate": "latest",
    "@formkit/core": "1.3.1",
    "@formkit/inputs": "1.3.1",
    "@formkit/utils": "1.3.1"
  },
  "devDependencies": {
    "@formkit/vue": "^1.3.0"
  }
}