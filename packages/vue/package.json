{
  "name": "@formkit/vue",
  "version": "1.6.0",
  "type": "module",
  "description": "Build industry leading Vue forms 10x faster.",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "development": "./dist/index.dev.mjs",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "iife": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.iife.js"
      }
    }
  },
  "keywords": [
    "vue",
    "forms",
    "inputs",
    "validation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/formkit/formkit.git",
    "directory": "packages/rules"
  },
  "contributors": [
    "Justin Schroeder <justin@formkit.com>"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "@formkit/core": "workspace:*",
    "@formkit/dev": "workspace:*",
    "@formkit/i18n": "workspace:*",
    "@formkit/inputs": "workspace:*",
    "@formkit/observer": "workspace:*",
    "@formkit/rules": "workspace:*",
    "@formkit/themes": "workspace:*",
    "@formkit/utils": "workspace:*",
    "@formkit/validation": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@formkit/icons": "workspace:*"
  }
}
